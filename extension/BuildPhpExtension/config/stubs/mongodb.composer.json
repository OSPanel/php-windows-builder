{
    "require":
    {
        "ext-apcu": "*",
        "openssl": "*",
        "zlib": "*",
        "ext-zstd": "*",
        "libsasl": "*"
    },
    "php-ext":
    {
        "configure-options": [
        {
            "name": "enable-mongodb"
        },
        {
            "name": "with-mongodb-sasl=yes"
        },
        {
            "name": "with-mongodb-client-side-encryption=yes"
        }]
    }
}